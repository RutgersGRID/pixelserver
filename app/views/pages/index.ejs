<!DOCTYPE html>
<html>

<head>
  <title> Pixel </title>

  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

  <link rel="stylesheet" type="text/css" href="/main.bundle.css" media="screen" />
  <link rel="stylesheet" type="text/css" href="/pixelPage.bundle.css" media="screen" />
  <link rel="stylesheet" type="text/css" href="/pixelApp.bundle.css" media="screen" />

  <script src="https://use.fontawesome.com/39ddb2bf02.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Oswald:700" rel="stylesheet">
</head>

<body>
  <div class="topLayer">
    <header id="appTitle">
      <img src="/img/PixelWallLogo.png" width="42px"/>
      <h1>PXL WALL</h1>
    </header>
  </div>

  <article class="mainContent">
    <section id="view">
      <canvas id="editor" width="50" height="50"></canvas>
    </section>
    <section class="actionsWrapper">
      <section id="colorPalette">
        <div class="color color-0 top-left"></div>  
        <div class="color color-1"></div>  
        <div class="color color-2"></div>  
        <div class="color color-3"></div>  
        <div class="color color-4 active"></div>  
        <div class="color color-5"></div>  
        <div class="color color-6 top-right"></div>  
        <div class="color color-7 bottom-left"></div>  
        <div class="color color-8"></div>  
        <div class="color color-9"></div>  
        <div class="color color-10"></div>  
        <div class="color color-11"></div>  
        <div class="color color-12"></div>  
        <div class="select select-color bottom-right"></div>  
      </section>
    </section>
  <article>

  <script type="text/javascript" src="main.bundle.js"></script>
  <script type="text/javascript" src="pixelPage.bundle.js"></script>
  <script type="text/javascript" src="pixelApp.bundle.js"></script>
</body>

</html>
